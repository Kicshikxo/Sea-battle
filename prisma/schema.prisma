generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "postgresql"
    url      = env("DATABASE_URL")
}

model User {
    id       String @id() @default(dbgenerated("gen_random_uuid()")) @map("user_id") @db.Uuid
    socketId String @unique @map("socket_id")

    @@map("users")
}
